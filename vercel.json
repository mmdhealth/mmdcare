{
  "routes": [
    {
      "src": "/api/session-code/(stage|doctor)",
      "dest": "/api/session-code?slug=$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/mobile-upload",
      "dest": "/mobile-upload.html"
    },
    {
      "src": "/mobile-upload.html",
      "dest": "/mobile-upload.html"
    },
    {
      "src": "/",
      "dest": "/login.html"
    }
  ]
}

